type HttpResponse = 
 record {
   body: nat;
   headers: vec HeaderField;
   status: nat16;
 };
type HttpRequest = 
 record {
   body: blob;
   headers: vec HeaderField;
   methods: vec text;
   url: vec text;
 };
type HeaderField = 
 record {
   text;
   text;
 };
service : {
  http_request: (HttpRequest) -> (HttpResponse) query;
  inc: () -> ();
  set: (nat) -> ();
}
